<!DOCTYPE html>
<html>
<head>
    <title>{content.siteTitle}</title>
    {>"shared/head"/}
</head>
<body>
    {>"shared/nav"/}

    <div class="container">
        <div class="page-header">
            <h1>{content.title}</h1>
        </div>
        <div id="introduction">
            <p class="lead" id="intro">{content.intro}</p>

            <button type="button" class="btn btn-primary" id="startTest" disabled>Start</button>
        </div>

        <form id="test" style="display:none" class="form-horizontal">
            <h3 id="questionLabel"></h3>
            <div class="checkbox">
                <label>
                    <input type="checkbox" name="question1" id="question1-input" value="checked">
                    <span id="question1-label"></span>
                </label>
                <br />
                <label>
                    <input type="checkbox" name="question2" id="question2-input" value="checked">
                    <span id="question2-label"></span>
                </label>
                <br />
                <label>
                    <input type="checkbox" name="question3" id="question3-input" value="checked">
                    <span id="question3-label"></span>
                </label>
                <br />
                <label>
                    <input type="checkbox" name="question4" id="question4-input" value="checked">
                    <span id="question4-label"></span>
                </label>
                <br />
                <br />
                <button type="submit" class="btn btn-primary" id="submitAnswer">Next</button>
            </div>

        </form>
    </div>

    {>"shared/footer"/}

    <script type="text/javascript" src="/public/js/parallel_programming/practice.js"></script>
</body>
</html>